{
  "name": "any-volt-backend",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
    "scripts": {
    "dev": "npm-run-all -s seed:strapi dev:app",
    "dev:app": "npm run develop",

    "develop": "strapi develop",
    "build": "strapi build",
    "start": "strapi start",
    "strapi": "strapi",

    "db:up": "docker compose up -d",
    "db:down": "docker compose down",

    "migrate:make": "knex --knexfile ./knexfile.cjs migrate:make",
    "migrate:up": "knex --knexfile ./knexfile.cjs migrate:latest",
    "migrate:down": "knex --knexfile ./knexfile.cjs migrate:rollback --all",
    "migrate:status": "node -e \"const k=require('knex')(require('./knexfile.cjs'));k.migrate.list().then(([done,pending])=>{console.log('Done:',done.map(m=>m.file));console.log('Pending:',pending.map(m=>m.file));}).finally(()=>process.exit());\"",

    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry",

    "seed:run": "node ./scripts/seed.js",
    "seed:strapi": "node scripts/seedProducts.strapi.cjs",

    "db:migrate": "knex migrate:latest --knexfile knexfile.cjs",
    "db:seed": "knex seed:run --knexfile knexfile.cjs",
    "db:reset": "npm run db:migrate && npm run seed:strapi",

    "postinstall": "npm run seed:strapi"
  },

  "dependencies": {
    "@strapi/plugin-cloud": "5.25.0",
    "@strapi/plugin-users-permissions": "5.25.0",
    "@strapi/strapi": "5.25.0",
    "better-sqlite3": "11.3.0",
    "mysql2": "^3.15.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv": "^17.2.3",
    "knex": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "cc2b39b0-15b1-49d9-9186-ad100c1306f9",
    "installId": "6662668df64c8e37d2ff9ab74a9c4455b1cf15e2adf884ca3436329c96277a2d"
  }
}
