{
  "kind": "collectionType",
  "collectionName": "embedded_videos",
  "info": {
    "singularName": "embedded-video",
    "pluralName": "embedded-videos",
    "displayName": "Embedded Video"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "videoUrl": {
      "type": "string",
      "required": true
    },
    "platform": {
      "type": "enumeration",
      "default": "youtube",
      "enum": [
        "youtube",
        "vimeo",
        "other"
      ]
    },
    "Media": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    }
  }
}
